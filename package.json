{
    "name": "grapesjs-bootstrap-elements",
    "version": "0.0.4",
    "description": "GrapesJS Bootstrap Elements",
    "main": "dist/grapesjs-bootstrap-elements.js",
    "scripts": {
        "lint": "eslint src",
        "v:patch": "npm version --no-git-tag-version patch",
        "v:minor": "npm version --no-git-tag-version minor",
        "build": "npm run v:patch && webpack --env production",
        "build-dev": "npm run && webpack --env production",
        "start": "node src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pepiuox/grapesjs-bootstrap-elements.git"
    },
    "keywords": [
        "grapesjs",
        "bootstrap",
        "elements",
        "components",
        "layouts",
        "plugin",
        "boilerplate",
        "wysiwyg"
    ],
    "author": "Jose Mantilla",
    "license": "BSD-3-Clause",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
        "@babel/preset-env": "^7.15.6",
        "@webpack-cli/init": "^1.1.3",
        "babel-loader": "^8.2.2",
        "eslint": "^7.32.0",
        "html-webpack-plugin": "^5.3.2",
        "raw-loader": "^4.0.2",
        "terser-webpack-plugin": "^5.1.1",
        "webpack": "^5.55.1",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.3.0"
    },
    "dependencies": {
        "npm": "^7.24.1"
    }
}
